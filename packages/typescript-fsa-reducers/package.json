{
  "name": "@diamondyuan/typescript-fsa-reducers",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Fluent syntax for defining typesafe Redux reducers on top of typescript-fsa.",
  "main": "dist/index.js",
  "types": "dist/index",
  "files": [
    "dist/"
  ],
  "keywords": [
    "redux",
    "typescript",
    "action",
    "reducer"
  ],
  "scripts": {
    "build": "yarn run clean && tsc -p tsconfig.build.json",
    "clean": "rm -rf dist/*",
    "format-file": "prettier --write",
    "format": "git ls-files | egrep '\\.(js(on)?|scss|tsx?)?$' | xargs yarn run format-file",
    "generate-toc": "git ls-files | egrep '\\.md$' | xargs scripts/markdown-toc-all.sh",
    "jest": "jest",
    "lint-file": "tslint",
    "prepublishOnly": "yarn run test && yarn run build",
    "test": "yarn run lint && tsc && yarn run jest"
  },
  "devDependencies": {
    "@diamondyuan/typescript-fsa": "^3.0.0-beta-2",
    "@types/jest": "^23.3.3",
    "jest": "^23.6.0",
    "markdown-toc": "^1.2.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.3",
    "typescript": "^3.1.1"
  },
  "peerDependencies": {
    "typescript-fsa": "*"
  }
}
