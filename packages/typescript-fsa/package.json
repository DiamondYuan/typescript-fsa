    "prepublish": "npm run clean && npm run build"
  },
  "author": "Daniel Lytkin <dan.lytkin@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.3"
  }
}
