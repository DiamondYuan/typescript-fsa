{
  "name": "@diamondyuan/typescript-fsa",
  "version": "0.0.0",
  "description": "Type-safe action creator utilities",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "redux",
    "flux",
    "typescript",
    "action",
    "action creator"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "test": "ts-node -P tsconfig.tests.json node_modules/.bin/tape tests/*.ts",
    "build:commonjs": "tsc",
    "prepublish": "npm run clean && npm run test && npm run build"
  },
  "author": "DiamondYuan",
  "license": "MIT",
  "devDependencies": {
    "@alexlur/rollup-plugin-typescript": "^1.0.4",
    "@types/tape": "^4.2.28",
    "rimraf": "^2.5.4",
    "rollup": "^0.46.0",
    "rollup-plugin-execute": "^1.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "tape": "^4.6.2",
    "ts-node": "^6.0.0",
    "typescript": "^2.8.1",
    "typings-tester": "^0.3.0"
  }
}
