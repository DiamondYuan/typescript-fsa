{
  "name": "redux-typescript-actions",
  "version": "1.0.0",
  "description": "Type-safe action creator utilities",
  "keywords": [
    "redux"
  ],
  "main": "lib/index.js",
  "jsnext:main": "es6/index.js",
  "typings": "./index.d.ts",
  "files": [
    "es6",
    "lib",
    "index.d.ts"
  ],
  "repository": "aikoven/redux-typescript-actions",
  "scripts": {
